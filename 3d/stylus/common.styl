            padding 3vh 0
            max-width 100%
            max-height 100%
            width 100%
            height 100%
            flexBox()
            flex-direction column
            justify-content space-around
            border 1px dashed #179a99
            border-radius 15px
              
        li
          font-family pilar-pro, sans-serif
          font-weight 400
          font-style normal
