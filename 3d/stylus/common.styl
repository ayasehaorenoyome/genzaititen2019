  z-index 999!important
  +MQ(SP_RES_WID)
    position fixed
    top 0
    border-bottom 1px solid #179a99
    background-size auto 8px
    height 78px
  .header_inner
    margin-top 23px
    height 100px
    width 80vw
    flexBox()
    justify-content space-between
    align-items center
    +MQ(SP_RES_WID)
      width 90vw
      margin-top 8px
      height 70px
    h1
